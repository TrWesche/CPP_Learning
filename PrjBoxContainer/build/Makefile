# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  BoxContainer_config = debug

else ifeq ($(config),release)
  BoxContainer_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := BoxContainer

.PHONY: all clean help $(PROJECTS) Core

all: $(PROJECTS)

Core: BoxContainer

BoxContainer:
ifneq (,$(BoxContainer_config))
	@echo "==== Building BoxContainer ($(BoxContainer_config)) ===="
	@${MAKE} --no-print-directory -C BoxContainer -f Makefile config=$(BoxContainer_config)
endif

clean:
	@${MAKE} --no-print-directory -C BoxContainer -f Makefile clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   BoxContainer"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"